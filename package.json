{
  "name": "react-ssr-boilerplate",
  "version": "1.0.0",
  "description": "React Server Side Rendering Boilerplate",
  "main": "app.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "dev:build-server": "NODE_ENV=development webpack --mode=production --config webpack.server.js --watch",
    "dev:build-client": "NODE_ENV=development webpack --mode=production --config webpack.client.js --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@loadable/server": "^5.14.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.14.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.17.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "webpack": "^5.12.3",
    "webpack-cli": "^4.3.1",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^2.5.2"
  }
}
